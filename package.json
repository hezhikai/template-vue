{
  "name": "template-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:js": "eslint --ext .js,.vue src",
    "fix:js": "eslint --fix --ext .js,.vue src",
    "lint:html": "htmlhint --config .htmlhintrc **/*.vue",
    "lint:style": "stylelint --config .stylelintrc.json **/*.{vue,css,scss,less}",
    "fix:style": "stylelint --config .stylelintrc.json **/*.{vue,css,scss,less} --fix",
    "prettier": "prettier --check **/*.{vue,css,scss,less,js,json}",
    "prettier:fix": "prettier --write **/*.{vue,css,scss,less,js,json}",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.0",
    "eslint-config-prettier": "^7.2.0",
    "js-cookie": "^2.2.1",
    "register-service-worker": "^1.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "htmlhint": "^0.14.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node verify-commit-msg.js"
  },
  "lint-staged": {
    "**/*.{js,vue,css,scss,less}": [
      "npm run fix:js",
      "npm run fix:style",
      "git add ."
    ],
    "**/*.{js,vue,css,scss,less,json}": [
      "npm run prettier:fix",
      "git add ."
    ]
  }
}
